<resources>
    
    <style name="MenuItemRippleStyle" parent="WaveRippleDrawableDefault">
        <item name="rd_rippleColor">#902DA0FF</item>
        <item name="rd_rippleAnimDuration">300</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">0dp</item>
    </style>

    <!-- AppBar Style -->
    
    <style name="AppBar" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="selectableItemBackground">@null</item>
    </style>
    
    <!-- Navigator Drawable -->
    
    <style name="NavigatorDrawableIcon" parent="NavigationDrawerIconDefault">
        <item name="lmd_strokeSize">3dp</item>
        <item name="lmd_padding">18dp</item>
    </style>
            
    <style name="NavigationDrawerDrawable">
        <item name="nd_ripple">@style/ToolbarRippleStyle</item>
        <item name="nd_icon">@style/NavigatorDrawableIcon</item>
    </style>
    
    <style name="ToolbarRippleStyle" parent="TouchRippleDrawableDefault">
        <item name="rd_backgroundColor">#00000000</item>
        <item name="rd_maxRippleRadius">48dp</item>
        <item name="rd_rippleColor">#33FFFFFF</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">0dp</item>
    </style>
    
    <!-- TabPageIndicator -->
    
    <style  name="TabPageIndicator"  parent="TabPageIndicatorDefault">
        <item name="tpi_tabPadding">12dp</item>
        <item name="tpi_tabRipple">@style/TabRippleStyle</item>
        <item name="tpi_indicatorHeight">3dp</item>
        <item name="android:textAppearance">@style/TabTextAppearance</item>
        <item name="tpi_mode">scroll</item>
    </style>
    
    <style name="TabTextAppearance" parent="@style/Base.TextAppearance.AppCompat.Subhead">
        <item name="android:textColor">@drawable/color_tpi</item>
    </style>
    
    <style name="TabRippleStyle" parent="WaveRippleDrawableDefault">
        <item name="rd_background">@null</item>
        <item name="rd_rippleColor">#90EAFB00</item>
        <item name="rd_rippleAnimDuration">300</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">0dp</item>
        <item name="rd_padding">0dp</item>
    </style>
    
    <!-- Progress Style -->
    
    <style name="CircularProgressDrawableStyle" parent="CircularProgressDrawableDefault">
        <item name="cpd_strokeColor">#FF4389F3</item>
    </style>
    
    <style name="ColorsCircularProgressDrawableStyle" parent="CircularProgressDrawableDefault">
        <item name="cpd_strokeColors">@array/progress_colors</item>
    </style>
    
    <style name="InOutCircularProgressDrawableStyle" parent="CircularProgressDrawableDefault">
        <item name="cpd_inAnimDuration">800</item>
        <item name="cpd_inStepPercent">0.5</item>
        <item name="cpd_inStepColors">@array/in_colors</item>
        <item name="cpd_outAnimDuration">400</item>
    </style>
    
    <style name="InOutColorsCircularProgressDrawableStyle" parent="CircularProgressDrawableDefault">
        <item name="cpd_inAnimDuration">800</item>
        <item name="cpd_inStepPercent">0.5</item>
        <item name="cpd_inStepColors">@array/in_colors</item>
        <item name="cpd_outAnimDuration">400</item>
        <item name="cpd_strokeColors">@array/progress_colors</item>
    </style>
    
    <style name="DeterminateInOutCircularProgressDrawableStyle" parent="CircularProgressDrawableDefault">
        <item name="cpd_inAnimDuration">400</item>
        <item name="cpd_outAnimDuration">400</item>
        <item name="cpd_initialAngle">-90</item>
    </style>
    
    <style name="DeterminateCircularProgressDrawableStyle" parent="CircularProgressDrawableDefault">
        <item name="cpd_inAnimDuration">0</item>
        <item name="cpd_outAnimDuration">0</item>
        <item name="cpd_initialAngle">-90</item>
    </style>
    
    <style name="LinearProgressDrawableStyle" parent="LinearProgressDrawableDefault">
        <item name="lpd_strokeColor">#FF4389F3</item>
        <item name="lpd_strokeSecondaryColor">#00000000</item>
    </style>
    
    <style name="ColorsLinearProgressDrawableStyle" parent="LinearProgressDrawableDefault">
        <item name="lpd_strokeColors">@array/progress_colors</item>
        <item name="lpd_strokeSecondaryColor">#00000000</item>
    </style>
    
    <style name="DeterminateLinearProgressDrawableStyle" parent="LinearProgressDrawableDefault">
        <item name="lpd_strokeColor">#FF4389F3</item>
        <item name="lpd_strokeSecondaryColor">#FFB6CDF2</item>
    </style>
    
    <style name="QueryLinearProgressDrawableStyle" parent="LinearProgressDrawableDefault">
        <item name="lpd_strokeColor">#FF4389F3</item>
        <item name="lpd_strokeSecondaryColor">#FFB6CDF2</item>
    </style>
    
    <style name="BufferLinearProgressDrawableStyle" parent="LinearProgressDrawableDefault">
        <item name="lpd_travelDuration">400</item>
        <item name="lpd_transformDuration">400</item>
        <item name="lpd_keepDuration">100</item>
    </style>
    
    
    
    <!-- Button Style -->
    
    
    <style name="FlatButtonRippleStyle" parent="TouchRippleDrawableDefault">
        <item name="rd_backgroundColor">#33999999</item>
        <item name="rd_maxRippleRadius">48dp</item>
        <item name="rd_rippleColor">#33999999</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">4dp</item>
    </style>
    
    <style name="FlatWaveButtonRippleStyle" parent="WaveRippleDrawableDefault">
        <item name="rd_rippleColor">#90999999</item>
        <item name="rd_rippleAnimDuration">300</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">4dp</item>
    </style>
    
    <style name="FlatColorButtonRippleStyle" parent="TouchRippleDrawableDefault">
        <item name="rd_backgroundColor">#33219AFF</item>
        <item name="rd_maxRippleRadius">48dp</item>
        <item name="rd_rippleColor">#33219AFF</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">4dp</item>
    </style>
    
    <style name="FlatWaveColorButtonRippleStyle" parent="WaveRippleDrawableDefault">
        <item name="rd_rippleColor">#902DA0FF</item>
        <item name="rd_rippleAnimDuration">300</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">4dp</item>
    </style>
    
    <style name="RaiseButtonRippleStyle" parent="TouchRippleDrawableDefault">
        <item name="rd_background">@drawable/bg_bt_raise</item>
        <item name="rd_backgroundColor">#33999999</item>
        <item name="rd_maxRippleRadius">48dp</item>
        <item name="rd_rippleColor">#33999999</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">2dp</item>
        <item name="rd_leftPadding">1dp</item>
        <item name="rd_topPadding">1dp</item>
        <item name="rd_rightPadding">1dp</item>
        <item name="rd_bottomPadding">2dp</item>
    </style>
    
    <style name="RaiseWaveButtonRippleStyle" parent="WaveRippleDrawableDefault">
        <item name="rd_background">@drawable/bg_bt_raise</item>
        <item name="rd_rippleColor">#90999999</item>
        <item name="rd_rippleAnimDuration">300</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">2dp</item>
        <item name="rd_leftPadding">1dp</item>
        <item name="rd_topPadding">1dp</item>
        <item name="rd_rightPadding">1dp</item>
        <item name="rd_bottomPadding">2dp</item>
    </style>
    
    <style name="RaiseColorButtonRippleStyle" parent="TouchRippleDrawableDefault">
        <item name="rd_background">@drawable/bg_bt_raise_color</item>
        <item name="rd_backgroundColor">#33000000</item>
        <item name="rd_maxRippleRadius">48dp</item>
        <item name="rd_rippleColor">#33000000</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">2dp</item>
        <item name="rd_leftPadding">1dp</item>
        <item name="rd_topPadding">1dp</item>
        <item name="rd_rightPadding">1dp</item>
        <item name="rd_bottomPadding">2dp</item>
    </style>
    
    <style name="RaiseWaveColorButtonRippleStyle" parent="WaveRippleDrawableDefault">
        <item name="rd_background">@drawable/bg_bt_raise_color</item>
        <item name="rd_rippleColor">#90EAFB00</item>
        <item name="rd_rippleAnimDuration">300</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">2dp</item>
        <item name="rd_leftPadding">1dp</item>
        <item name="rd_topPadding">1dp</item>
        <item name="rd_rightPadding">1dp</item>
        <item name="rd_bottomPadding">2dp</item>
    </style>
    
    <style name="FloatButtonRippleStyle" parent="TouchRippleDrawableDefault">
        <item name="rd_background">@drawable/bg_bt_float</item>
        <item name="rd_backgroundColor">#33999999</item>
        <item name="rd_maxRippleRadius">32dp</item>
        <item name="rd_rippleColor">#33999999</item>
        <item name="rd_maskType">oval</item>
        <item name="rd_leftPadding">2dp</item>
        <item name="rd_topPadding">2dp</item>
        <item name="rd_rightPadding">2dp</item>
        <item name="rd_bottomPadding">4dp</item>
    </style>
    
    <style name="FloatWaveButtonRippleStyle" parent="WaveRippleDrawableDefault">
        <item name="rd_background">@drawable/bg_bt_float</item>
        <item name="rd_rippleColor">#90999999</item>
        <item name="rd_rippleAnimDuration">300</item>
        <item name="rd_maskType">oval</item>
        <item name="rd_leftPadding">2dp</item>
        <item name="rd_topPadding">2dp</item>
        <item name="rd_rightPadding">2dp</item>
        <item name="rd_bottomPadding">4dp</item>
    </style>
    
	<style name="FloatColorButtonRippleStyle" parent="TouchRippleDrawableDefault">
        <item name="rd_background">@drawable/bg_bt_float_color</item>
        <item name="rd_backgroundColor">#33000000</item>
        <item name="rd_maxRippleRadius">32dp</item>
        <item name="rd_rippleColor">#33000000</item>
        <item name="rd_maskType">oval</item>
        <item name="rd_leftPadding">2dp</item>
        <item name="rd_topPadding">2dp</item>
        <item name="rd_rightPadding">2dp</item>
        <item name="rd_bottomPadding">4dp</item>
    </style>
    
	<style name="FloatWaveColorButtonRippleStyle" parent="WaveRippleDrawableDefault">
        <item name="rd_background">@drawable/bg_bt_float_color</item>
        <item name="rd_rippleColor">#90EAFB00</item>
        <item name="rd_rippleAnimDuration">300</item>
        <item name="rd_maskType">oval</item>
        <item name="rd_leftPadding">2dp</item>
        <item name="rd_topPadding">2dp</item>
        <item name="rd_rightPadding">2dp</item>
        <item name="rd_bottomPadding">4dp</item>
    </style>
    
	<!-- Switch Style -->
	
	<style name="RadioButtonDrawable" parent="RadioButtonDrawableDefault">
        <item name="rbd_width">32dp</item>
        <item name="rbd_height">32dp</item>
        <item name="rbd_strokeSize">2dp</item>
        <item name="rbd_radius">10dp</item>
        <item name="rbd_innerRadius">5dp</item>
        <item name="rbd_animDuration">300</item>
    </style>
    
    <style name="CheckBoxDrawable" parent="CheckBoxDrawableDefault">
        <item name="cbd_width">32dp</item>
        <item name="cbd_height">32dp</item>
        <item name="cbd_boxSize">20dp</item>
        <item name="cbd_cornerRadius">2dp</item>
        <item name="cbd_strokeSize">2dp</item>
        <item name="cbd_animDuration">300</item>
    </style>
    
    <!-- SnackBar Style -->
    <style name="SnackBar" parent="SnackBarDefault">
        <item name="sb_backgroundColor">#FF323232</item>
        <item name="sb_backgroundCornerRadius">0dp</item>
        <item name="sb_horizontalPadding">24dp</item>
        <item name="sb_verticalPadding">0dp</item>
        <item name="sb_width">match_parent</item>
        <item name="sb_height">48dp</item>        
        <item name="sb_textAppearance">@style/Base.TextAppearance.AppCompat.Body1</item>
        <item name="sb_actionTextAppearance">@style/Base.TextAppearance.AppCompat.Button</item>
        <item name="sb_actionTextColor">#FFF7FF41</item>
        <item name="sb_actionRipple">@style/MenuItemRippleStyle</item>
    </style>
    
    <!-- SnackBar Style -->
    
    <style name="FloatingActionButton" parent="FloatingActionButtonDefault">
        <item name="fab_radius">28dp</item>
        <item name="fab_elevation">4dp</item>
        <item name="fab_icon">@style/FloatingActionButtonIcon</item>
        <item name="fab_iconSize">24dp</item>
        <item name="fab_autoSwitch">true</item>
        <item name="ripple">@style/RaiseWaveColorButtonRippleStyle</item>
        <item name="delayClick">false</item>
    </style>
    
    <style name="FloatingActionButtonIcon">
        <item name="lmd_state">@xml/fab_icon_states</item>
        <item name="lmd_curState">0</item>
        <item name="lmd_padding">2dp</item>
        <item name="lmd_animDuration">400</item>
        <item name="lmd_interpolator">@android:anim/accelerate_decelerate_interpolator</item>
        <item name="lmd_strokeSize">3dp</item>
        <item name="lmd_strokeColor">#FFFFFFFF</item>
        <item name="lmd_strokeCap">butt</item>
        <item name="lmd_strokeJoin">mitter</item>
        <item name="lmd_clockwise">true</item>
    </style>

    <!-- Dialog -->

    <style name="SimpleDialog">
        <item name="android:layout_width">200dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="di_dimAmount">0.5</item>
        <item name="di_backgroundColor">#FFFFFFFF</item>
        <item name="di_elevation">8dp</item>
        <item name="di_maxElevation">8dp</item>
        <item name="di_cornerRadius">2dp</item>
        <item name="di_titleTextAppearance">@style/TextAppearance.AppCompat.Title</item>
        <item name="di_titleTextColor">#FF000000</item>
        <item name="di_actionRipple">@style/FlatWaveColorButtonRippleStyle</item>
        <item name="di_actionTextAppearance">@style/TextAppearance.AppCompat.Button</item>
        <item name="di_actionTextColor">#FF000000</item>
        <item name="di_messageTextAppearance">@style/TextAppearance.AppCompat.Body1</item>
        <item name="di_messageTextColor">#FF000000</item>
        <item name="di_dividerColor">#1E000000</item>
        <item name="di_dividerHeight">1dp</item>
        <item name="di_cancelable">true</item>
        <item name="di_canceledOnTouchOutside">true</item>
        <item name="di_radioButtonStyle">@style/DialogRadioButton</item>
        <item name="di_checkBoxStyle">@style/DialogCheckBox</item>
        <item name="di_itemTextAppearance">@style/DialogTextAppearance</item>
        <item name="di_itemHeight">48dp</item>
    </style>

    <style name="DialogTextAppearance" parent="TextAppearance.AppCompat.Body1">
        <item name="android:textColor">#FF000000</item>
    </style>

    <style name="DialogRadioButton" parent="RadioButtonDrawableDefault">
        <item name="rbd_width">22dp</item>
        <item name="rbd_height">22dp</item>
        <item name="rbd_strokeSize">2dp</item>
        <item name="rbd_radius">10dp</item>
        <item name="rbd_innerRadius">5dp</item>
        <item name="rbd_animDuration">300</item>
    </style>

    <style name="DialogCheckBox" parent="CheckBoxDrawableDefault">
        <item name="cbd_width">22dp</item>
        <item name="cbd_height">22dp</item>
        <item name="cbd_boxSize">20dp</item>
        <item name="cbd_cornerRadius">2dp</item>
        <item name="cbd_strokeSize">2dp</item>
        <item name="cbd_animDuration">300</item>
    </style>

    <style name="TimePickerDialog" parent="SimpleDialog">
        <item name="tp_headerHeight">120dp</item>
        <item name="tp_backgroundColor">#FFECEFF1</item>
        <item name="tp_selectionColor">#FF009688</item>
        <item name="tp_selectionRadius">16dp</item>
        <item name="tp_tickSize">1dp</item>
        <item name="tp_textSize">14sp</item>
        <item name="tp_textTimeSize">24sp</item>
        <item name="android:textStyle">normal</item>
        <item name="tp_textColor">#FF000000</item>
        <item name="tp_textTimeColor">#90FFFFFF</item>
        <item name="tp_textHighlightColor">#FFFFFFFF</item>
        <item name="tp_animDuration">@android:integer/config_mediumAnimTime</item>
        <item name="tp_inInterpolator">@android:anim/decelerate_interpolator</item>
        <item name="tp_outInterpolator">@android:anim/decelerate_interpolator</item>
        <item name="tp_am">AM</item>
        <item name="tp_pm">PM</item>
    </style>
    
</resources>
